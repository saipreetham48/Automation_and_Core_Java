package sujithQns;

import java.util.Hashtable;
import java.util.Map;

public class Practice {

	public static void main(String[] args) {
		String str="Sajdheghcbchbhc";
		char[] ch=str.toCharArray();
		Map<Character, Integer> m=new Hashtable<Character, Integer>();
		for (Character c : ch) {
			m.put(c, m.getOrDefault(c,0)+1);
//			if(m.get(c)>1) {
//				System.out.println("First repeated char:"+c);
//				break;
//			}
		 
			
		}
		 System.out.println("Each char count is:"+m);//each char count
		 //System.out.print("Only duplicate chars");
		 //System.out.print("Other than duplicate chars");
		for (Character key : m.keySet()) {
//			if(m.get(key)>1)
//				System.out.print(" "+key);//only duplicates
//			if(m.get(key)<2)
//				System.out.print(" "+key);//other than duplicates
			
		}
		
	}

}
